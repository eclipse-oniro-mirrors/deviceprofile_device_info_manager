{
  "process": "deviceprofile",
  "systemability": [
      {
          "name": 6001,
          "libpath": "libdistributed_device_profile_svr.z.so",
          "run-on-create": false,
          "distributed": true,
          "dump-level": 1,
          "recycle-strategy": "low-memory",
          "start-on-demand": {
              "deviceonline": [
                  {
                      "name": "deviceonline",
                      "value": "on"
                  }
              ],
              "commonevent":[
                {
                  "name":"usual.event.BOOT_COMPLETED"
                }
              ]
          },
          "stop-on-demand": {
            "deviceonline": [
                {
                    "name": "deviceonline",
                    "value": "off"
                }
            ]
          }
      }
  ]
}